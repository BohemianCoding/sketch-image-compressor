{
  "name": "sketch-image-compressor",
  "version": "1.3.1",
  "description": "A Plugin that compresses bitmap assets, right when you export them. This Plugin *requires* Sketch 3.8.",
  "skpm": {
    "name": "Sketch Image Compressor",
    "main": "Sketch Image Compressor.sketchplugin",
    "manifest": "src/manifest.json",
    "assets": [
      "assets/**/*"
    ]
  },
  "main": "src/plugin.js",
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Ale Mu√±oz <ale@sketchapp.com>",
  "license": "MIT",
  "repository": "https://github.com/sketch-hq/sketch-image-compressor",
  "devDependencies": {
    "@skpm/builder": "^0.7.4"
  },
  "dependencies": {
    "@skpm/child_process": "^0.4.1",
    "@skpm/dialog": "^0.3.0",
    "@skpm/fs": "^0.2.5",
    "pretty-bytes": "^5.3.0",
    "pretty-ms": "^5.0.0"
  }
}
